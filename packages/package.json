{
  "name": "@xuanrushi/ssy-ui-vite",
  "type": "module",
  "version": "0.2.0",
  "description": "ssy-ui-template模版项目",
  "author": "xuanrushi",
  "license": "ISC",
  "homepage": "https://ssy-ui-vite.vercel.app/",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/ssy-ui.js",
      "require": "./dist/ssy-ui.umd.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/ssy-ui.umd.cjs",
  "module": "./dist/ssy-ui.js",
  "types": "./dist/entry.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "pnpm lint && pnpm test",
    "prepare": "husky",
    "build:all": "vite build",
    "build:components": "tsx ./scripts/build.ts",
    "build": "pnpm build:components && pnpm type:build",
    "typecheck": "vue-tsc --noEmit",
    "type:build": "vue-tsc -b --declaration --emitDeclarationOnly tsconfig.types.json",
    "clean:dist": "rimraf dist",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@unocss/eslint-plugin": "^0.62.4",
    "@unocss/vite": "^0.63.4",
    "clipboard": "^2.0.11",
    "eslint": "^9.13.0",
    "eslint-plugin-format": "^0.1.2",
    "highlight.js": "^11.10.0",
    "prismjs": "^1.29.0",
    "ssy-ui-vite": "^1.8.0",
    "typescript": "5.5.4",
    "unocss": "^0.63.4",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify-json/ic": "^1.2.1",
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-istanbul": "^2.1.4",
    "@vitest/coverage-v8": "^2.1.4",
    "@vue/test-utils": "2.4.6",
    "cz-customizable": "^7.2.1",
    "fs-extra": "^11.2.0",
    "happy-dom": "15.7.4",
    "husky": "latest",
    "terser": "5.34.1",
    "tsx": "^4.19.1",
    "unocss": "^0.63.4",
    "vite": "^5.4.10",
    "vitepress": "^1.4.1",
    "vitepress-theme-demoblock": "3.0.7",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
